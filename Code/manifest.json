{
  "name": "QuickNavigator",
  "version": "0.2",
  "manifest_version": 2,
  "description": "",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "background":{
	"page":"bg.html"
  },
   "options_page": "options.html",
   "icons": {
      "128": "images/icon.png",
      "16": "images/icon.png",
      "48": "images/icon.png"
   },  
  "content_scripts":[
  {
    "js":["util/jquery-1.7.2.min.js","util/angular/angular.js","util/angular/angular-sanitize.min.js","util/angular/app.services.js","util/angular/app.filters.js","util/angular/app.directives.js","util/options.js","util/config.js","util/pinyin-dict-combo.js","util/pinyin.js","content_scripts/omnibox.js","content_scripts/app.js"],
    "css":["css/app.css" ],
    "matches":["http://*/*","https://*/*"],
    "run_at":"document_start"
  }],
  "permissions": [
	"http://*/*",
	"https://*/*",
    "tabs",
    "bookmarks",
    "history"
    ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
